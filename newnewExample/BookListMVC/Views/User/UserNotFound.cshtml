@model int

@{
    ViewBag.Title= "UserNotFound";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error: </h4>
    <hr />
    <h5>
        User with Id = @Model cannot be found
    </h5>
</div>

<a asp-action="index" asp-controller="home" class="btn btn-outline-success" style="width: auto">
    Click here to see the list of all known users
</a>